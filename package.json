{
  "name": "ts-todo-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "ts-todo": "git+ssh://git@github.com:bretfunk/ts-todo#master"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --inline",
    "start:api": "NODE_ENV=api-development webpack-dev-server --hot --inline",
    "build:demo": "NODE_ENV=demo webpack",
    "build": "NODE_ENV=production webpack",
    "deploy:demo": "s3-website deploy -f .s3-website-demo.json",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint": "tslint -c tslint.json \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier --write 'src/**/*.{tsx,ts}'"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.8",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "typescript": "^2.8.3"
  }
}
